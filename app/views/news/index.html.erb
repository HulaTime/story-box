<table id='news'>

<iframe src="http://www.w3schools.com">
  <p>Your browser does not support iframes.</p>
</iframe>
	
	<% @news_sources.each_slice(5) do |slice| %>
		
		<tr>
			<% slice.each do |s| %>

				<td class='source-info'>
					<a href=<%=s["url"]%> >
 						<img src=<%=s['urlsToLogos']['small']%> alt="<%=s['name']%>" >
 					</a>
 					<p>
 						<%= s['description'] %>
 					</p>
				</td>

			<% end %>
		</tr>

	<% end %>

</table>

